<div class="page-header">
  <h3>Event Comment #<%= @event_comment.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">

      <dt>User</dt>
      <dd><%= @event_comment.user_id %></dd>


      <dt>Event</dt>
      <dd><%= @event_comment.event_id %></dd>


      <dt>Content</dt>
      <dd><%= @event_comment.content %></dd>


    </dl>

    <div class="btn-group btn-group-justified">

      <a href="/event_comments" class="btn btn-primary">
        Back
      </a>

      <a href="/event_comments/<%= @event_comment.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_event_comment/<%= @event_comment.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>


    </div>
  </div>
</div>
