<div class="page-header">
  <h3>Event #<%= @event.id %></h3>
</div>



<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">

      <dt>Title</dt>
      <dd><%= @event.title %></dd>


      <dt>Description</dt>
      <dd><%= @event.description %></dd>


      <dt>Street</dt>
      <dd><%= @event.street %></dd>


      <dt>City</dt>
      <dd><%= @event.city %></dd>


      <dt>State</dt>
      <dd><%= @event.state %></dd>


      <dt>Zip</dt>
      <dd><%= @event.zip %></dd>


      <dt>Neighborhood</dt>
      <dd><%= @event.neighborhood %></dd>


      <dt>Number of spots</dt>
      <dd><%= @event.number_of_spots %></dd>


      <dt>Date time</dt>
      <dd><%= @event.date_time %></dd>


      <dt>Image url</dt>
      <dd><%= @event.image_url %></dd>


      <dt>Public private</dt>
      <dd><%= @event.public_private %></dd>


      <dt>Flakiness bar</dt>
      <dd><%= @event.flakiness_bar %></dd>


      <dt>User</dt>
      <dd><%= @event.user_id %></dd>


    </dl>

    <div class="btn-group btn-group-justified">

      <a href="/events" class="btn btn-primary">
        Back
      </a>

      <a href="/events/<%= @event.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_event/<%= @event.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>


    </div>
  </div>
</div>
